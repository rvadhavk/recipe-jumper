{
  "manifest_version": 3,
  "name": "Recipe Jumper",
  "version": "1.0",
  "description": "Automatically jumps to the recipe on a web page.",
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"],
      "run_at": "document_end"
    }
  ]
}
